<div class="overflow-x-auto">
  <table class="table table-xs">
    <thead>
      <tr>
        <th></th> 
        <th>Nombre</th> 
        <th>Fecha</th> 
        <th>Hora</th>
      </tr>
    </thead>
    <tbody>
