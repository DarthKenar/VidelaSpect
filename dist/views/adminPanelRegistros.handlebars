                <td>{{this.hora}}</td>
            </tr>
        {{/each}}
    </tbody> 
    <tfoot>
      <tr>
        <th></th> 
        <th>Nombre</th> 
        <th>Fecha</th> 
        <th>Hora</th>
      </tr>
    </tfoot>
  </table>
